<platformUi:DialogWindow  
    x:Class="Alteridem.GitHub.Extension.View.AuthDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:viewModel="clr-namespace:Alteridem.GitHub.Extension.ViewModel"
    xmlns:local="clr-namespace:Alteridem.GitHub.Extension.View"
    xmlns:platformUi="clr-namespace:Microsoft.VisualStudio.PlatformUI;assembly=Microsoft.VisualStudio.Shell.11.0"
    mc:Ignorable="d" 
    WindowStyle="None" 
    WindowStartupLocation="CenterOwner" 
    Height="800" Width="640"
    d:DataContext="{d:DesignInstance viewModel:AuthViewModel}" >
    <Grid>
        <WebBrowser x:Name="browser" LoadCompleted="OnLoadCompleted" />
    </Grid>
</platformUi:DialogWindow>
